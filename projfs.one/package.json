{
  "name": "projfs.one",
  "version": "0.1.0",
  "description": "Windows ProjectedFS provider for ONE.core content-addressed storage",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=20.0.0"
  },
  "os": [
    "win32"
  ],
  "scripts": {
    "build": "npm run build:native && npm run build:ts",
    "build:native": "node-gyp rebuild",
    "build:ts": "tsc",
    "postbuild": "node scripts/post-build.mjs",
    "clean": "rimraf dist build",
    "test": "npm run build:ts && mocha \"dist/test/**/*.test.js\"",
    "test:unit": "npm run build:ts && mocha \"dist/test/unit/**/*.test.js\" --timeout 5000",
    "test:integration": "npm run build:ts && mocha \"dist/test/integration/**/*.test.js\" --timeout 30000",
    "test:performance": "npm run build:ts && mocha \"dist/test/performance/**/*.test.js\" --timeout 60000",
    "test:windows": "run-tests.cmd",
    "test:clean": "rimraf dist-test",
    "dev": "nodemon --watch src --exec 'npm run build:ts && node dist/examples/basic-mount.js'",
    "example": "node dist/examples/basic-mount.js",
    "install": "node-gyp-build"
  },
  "dependencies": {
    "@refinio/one.core": "file:../vendor/refinio-one.core-0.6.1-beta-1.tgz",
    "bindings": "^1.5.0",
    "node-addon-api": "^7.0.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/bindings": "^1.5.5",
    "@types/chai": "^4.3.11",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.0.0",
    "@types/sinon": "^17.0.2",
    "chai": "^4.3.10",
    "cross-env": "^10.0.0",
    "mocha": "^10.2.0",
    "node-gyp": "^10.0.1",
    "nodemon": "^3.0.2",
    "rimraf": "^5.0.5",
    "sinon": "^17.0.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.6.2",
    "typescript": "^5.3.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/refinio/one.filer.git"
  },
  "keywords": [
    "projfs",
    "projected-file-system",
    "windows",
    "filesystem",
    "virtual-filesystem",
    "content-addressed",
    "one-core"
  ],
  "author": "REFINIO GmbH",
  "license": "MIT",
  "gypfile": true
}
