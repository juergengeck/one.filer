{
    "name": "one.filer.windows",
    "version": "4.0.0-beta-1",
    "description": "ONE File System Bridge for Windows Explorer using ProjectedFS",
    "type": "module",
    "main": "lib/index.js",
    "bin": {
        "one-filer": "lib/index.js"
    },
    "scripts": {
        "start": "node --enable-source-maps lib/index.js",
        "build": "tsc && node scripts/post-build.mjs",
        "clean": "rimraf lib",
        "test": "node scripts/testing/test-cross-platform-refinio-cli.js",
        "test:linux": "bash scripts/testing/test-with-refinio-cli.sh",
        "test:windows": "powershell -ExecutionPolicy Bypass -File ./scripts/testing/test-windows-refinio-cli.ps1",
        "test:wsl": "bash scripts/testing/test-wsl.sh",
        "test:compile": "tsc --project test/tsconfig.json",
        "test:unit": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha 'test/unit/**/*.test.ts' --timeout 10000",
        "test:functional": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha 'test/functional/**/*.test.ts' --timeout 120000",
        "test:integration": "mocha 'test/cross-platform-integration.test.js' --timeout 120000",
        "test:coverage": "nyc --reporter=text --reporter=html npm run test",
        "test:watch": "mocha --require ts-node/register 'test/**/*.test.ts' --watch --watch-extensions ts"
    },
    "repository": "https://github.com/refinio/one.filer",
    "dependencies": {
        "@commander-js/extra-typings": "^10.0.3",
        "@refinio/one.core": "file:vendor/refinio-one.core-0.6.1-beta-1.tgz",
        "@refinio/one.models": "file:vendor/refinio-one.models-14.1.0-beta-1.tgz",
        "commander": "^10.0.0",
        "fuse-shared-library-linux": "1.0.0",
        "rimraf": "^4.1.2"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.11",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.0.0",
        "@types/sinon": "^17.0.2",
        "chai": "^4.3.10",
        "chai-as-promised": "^7.1.1",
        "html-webpack-plugin": "^5.6.3",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "sinon": "^17.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^4.9.5",
        "webpack-cli": "^6.0.1"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "keywords": [
        "fuse",
        "filesystem",
        "wsl2",
        "windows-explorer",
        "one-database"
    ],
    "author": "REFINIO GmbH <support@refinio.net>",
    "license": "MIT",
    "homepage": "https://github.com/REFINIO/one.filer#readme",
    "overrides": {
        "lru-cache": "^11.1.0"
    }
}
