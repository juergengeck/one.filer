{
    "name": "one.filer",
    "version": "4.0.0-beta-1",
    "description": "ONE File System Bridge for Windows Explorer via WSL2",
    "type": "module",
    "main": "lib/index.js",
    "bin": {
        "one-filer": "lib/index.js"
    },
    "scripts": {
        "start": "node --enable-source-maps lib/index.js",
        "build": "tsc && node scripts/post-build.mjs",
        "clean": "rimraf lib",
        "test": "npm run test:unit && npm run test:integration",
        "test:unit": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha 'test/unit/**/*.test.ts' --timeout 10000",
        "test:integration": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha 'test/integration/**/*.test.ts' --timeout 30000",
        "test:functional": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha 'test/functional/**/*.test.ts' --timeout 120000",
        "test:functional:fuse": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha 'test/functional/fuse-linux.test.ts' --timeout 120000",
        "test:functional:windows": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha 'test/functional/fuse-windows-access.test.ts' --timeout 120000",
        "test:fuse:windows": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha 'test/functional/fuse-windows-native.test.ts' --timeout 120000",
        "test:fuse:crossplatform": "node --loader ts-node/esm --experimental-specifier-resolution=node node_modules/mocha/bin/mocha 'test/functional/fuse-cross-platform.test.ts' --timeout 120000",
        "test:all": "npm run test:unit && npm run test:integration && npm run test:functional",
        "test:coverage": "nyc --reporter=text --reporter=html npm run test",
        "test:wsl": "bash test-wsl.sh",
        "test:windows": "powershell -ExecutionPolicy Bypass -File ./test/scripts/run-windows-tests.ps1",
        "test:minimal": "node --enable-source-maps test/integration/02-minimal-fuse-example.test.js",
        "test:watch": "mocha --require ts-node/register 'test/**/*.test.ts' --watch --watch-extensions ts"
    },
    "repository": "https://github.com/refinio/one.filer",
    "dependencies": {
        "@commander-js/extra-typings": "^10.0.3",
        "@refinio/one.core": "file:./vendor/refinio-one.core-0.6.1-beta-1.tgz",
        "@refinio/one.models": "file:./vendor/refinio-one.models-14.1.0-beta-1-fixed.tgz",
        "commander": "^10.0.0",
        "fuse-shared-library-linux": "1.0.0",
        "rimraf": "^4.1.2"
    },
    "devDependencies": {
        "@istanbuljs/nyc-config-typescript": "^1.0.2",
        "@types/chai": "^4.3.11",
        "@types/mocha": "^10.0.6",
        "@types/node": "^20.0.0",
        "@types/sinon": "^17.0.2",
        "chai": "^4.3.10",
        "chai-as-promised": "^7.1.1",
        "mocha": "^10.2.0",
        "nyc": "^15.1.0",
        "sinon": "^17.0.1",
        "ts-node": "^10.9.2",
        "typescript": "^4.9.5"
    },
    "engines": {
        "node": ">=20.0.0"
    },
    "keywords": [
        "fuse",
        "filesystem",
        "wsl2",
        "windows-explorer",
        "one-database"
    ],
    "author": "REFINIO GmbH <support@refinio.net>",
    "license": "MIT",
    "homepage": "https://github.com/REFINIO/one.filer#readme",
    "overrides": {
        "lru-cache": "^11.1.0"
    }
}
