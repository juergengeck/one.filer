{
    "all": true,
    "check-coverage": true,
    "lines": 80,
    "statements": 80,
    "functions": 80,
    "branches": 80,
    "include": [
        "src/**/*.ts"
    ],
    "exclude": [
        "**/*.test.ts",
        "**/*.spec.ts",
        "**/*.d.ts",
        "test/**",
        "lib/**",
        "node_modules/**"
    ],
    "extension": [
        ".ts"
    ],
    "require": [
        "ts-node/register"
    ],
    "reporter": [
        "text",
        "lcov",
        "html"
    ],
    "sourceMap": true,
    "instrument": true
}